13 264
"Button5"
""
1 367 16 E "main()
{
  int i;
  float	 xRange;
  dyn_float       y;

  getValue( \"balken\", \"xRange\", xRange );

      y[1] = 10.0;
      for(i = 2; i <= 4; i++ )
      {
        y[i] = y[i-1] + i;
      }
      for(i = 5; i <= 8; i++ )
      {
        y[i] = y[i-1] - 2.0;
      }
      for(i = 9; i <= 20; i++ )
      {
        y[i] = y[i-1] + 2.0;
      }
      for(i = 21; i <= xRange; i++ )
      {
        y[i] = y[i-1] - 2.0;
      }

  dpSet( \"ExampleDP_bt.y1:_original.._value\", y);

}" 0

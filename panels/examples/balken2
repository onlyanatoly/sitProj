"main()
{
  int i;
  float	 xRange;
  dyn_float       y1;

  getValue( \"balken\", \"xRange\", xRange );

      y1[1] = 11.2;
      for(i = 2; i <= 6; i++ )
      {
         y1[i] = 25. - (rand()/32767.)*10.0;
      }
      for(i = 7; i <= 26; i++ )
      {
         y1[i] = 25. - (rand()/32767.)*10.0;

     }
     for(i = 27; i <= xRange; i++ )
     {
       y1[i] = 25. - (rand()/32767.)*10.0;
     }

  dpSet( \"ExampleDP_bt.y1:_original.._value\", y1 );

}" 0

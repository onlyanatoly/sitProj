E "bool flag = false;

main(int button)
{
  Chart_ewo2.updatesEnabled = false;
  setVisiblity(button);

  VisibilitySeries1.state(0, true);
  switch ( button )
  {
    case 0: // Donut
      if ( Chart_ewo2.chartType != \"Donut\" )
        clearAll();

      Chart_ewo2.chartType = \"Donut\";
      Chart_ewo2.shownValueText = \"Percent\";

      initRadar();
      break;

    case 1: // DonutBars
      Chart_ewo2.chartType = \"DonutBars\";
      Chart_ewo2.shownValueText = \"None\";
      Chart_ewo2.removeAxes();

      dyn_float values;
      int r = rand();

      for (int i = 0; i < 36; i++)
        values[i + 1] = 100 + sin(deg2rad((r + i * 10) % 180)) * 1000;

      Chart_ewo2.setSeries(0, values);

      if ( Chart_ewo2.isSeriesVisible(1) )
      {
        Chart_ewo2.setSeriesVisible(1, false);
        VisibilitySeries1.state(0, false);
      }

      if ( (Chart_ewo2.seriesCount() > 2) &&
            Chart_ewo2.isSeriesVisible(2) )
        Chart_ewo2.setSeriesVisible(2, false);

      int idx = 1;
      for (int i = 0; i < 36; i++)
      {
        idx = idx >= 11 ? 1 : idx;
        string color = colors[idx++];
        Chart_ewo2.setItemFillColor(0, i, color);
      }
      break;

    case 2: // Radar
      Chart_ewo2.chartType = \"Radar\";
      initRadar();

      if ( VisibilitySeries0.state(0) )
        Chart_ewo2.setSeriesVisible(0, true);
      else
        Chart_ewo2.setSeriesVisible(0, false);

      if ( VisibilitySeries1.state(0) )
        Chart_ewo2.setSeriesVisible(1, true);
      else
        Chart_ewo2.setSeriesVisible(0, false);

      if ( (Chart_ewo2.seriesCount() > 2) &&
            Chart_ewo2.isSeriesVisible(2) )
        Chart_ewo2.setSeriesVisible(2, false);

      Chart_ewo2.setSeriesName(0, \"Profit\");
      Chart_ewo2.setSeriesName(1, \"Sales\");
      Chart_ewo2.setSeriesFillColor(0, \"{255,185,0,100}\");
      Chart_ewo2.setSeriesFillColor(1, \"{175,35,95,100}\");
      break;

    case 3: // Bars
    case 4: // StackedBars
    case 5: // PercentStackedBars
      if ( button != 5 )
        Chart_ewo2.shownValueText = \"Value\";

      if ( button == 3 )
        Chart_ewo2.chartType = \"Bars\";
      else if ( button == 4 )
        Chart_ewo2.chartType = \"StackedBars\";
      else
      {
        Chart_ewo2.chartType = \"PercentStackedBars\";
        Chart_ewo2.shownValueText = \"Percent\";
      }

      for (int j = 0; j < 3; j++)
      {
        dyn_float values;
        dyn_string labels;

        for (int i = 1; i <= 6; i++)
        {
          values[i] = rand();
          labels[i] = formatTime(\"%b\", makeTime(2017, i, 1));
        }

        Chart_ewo2.setSeries(j, values, labels);
        Chart_ewo2.setSeriesVisible(j, true);
        Chart_ewo2.setSeriesName(j, \"series_\" + (string)j);
        Chart_ewo2.setSeriesFillColor(j, colors[j+3]);
      }
      Chart_ewo2.removeAxes();
      Chart_ewo2.addAxis(0, 0, \"[EURO]\");
      Chart_ewo2.addAxis(0, 0, \"Values Last Year\");
      break;

    case 6:  // Bubbles
      Chart_ewo2.chartType = \"Bubbles\";

      if ( VisibilitySeries1.state(0) )
        Chart_ewo2.setSeriesVisible(1, true);
      else
        Chart_ewo2.setSeriesVisible(0, false);

      if ( Chart_ewo2.seriesCount > 2 )
        Chart_ewo2.setSeriesVisible(2, false);

      Chart_ewo2.removeAxes();
      Chart_ewo2.addAxis(0, 0, \"[EURO]\");
      Chart_ewo2.addAxis(0, 0, \"Values Last Year\");

      for (int j = 0; j < 2; j++)
      {
        dyn_float dx, dy, dv;
        float variationX = 180.0 * rand() / 32768.0;
        float variationY = 180.0 * rand() / 32768.0;

        if ( Chart_ewo2.bubbleColorFromValue )
        {
          const int maxX = 40;
          const int maxY = 20;
          dx[maxX*maxY] = 0.0;
          dy[maxX*maxY] = 0.0;
          dv[maxX*maxY] = 0.0;

          for (int x = 0; x < maxX; x++)
          {
            for (int y = 0; y < maxY; y++)
            {
              dx[x * maxY + y + 1] = x;
              dy[x * maxY + y + 1] = y;

              dv[x * maxY + y + 1] = fabs(sin(deg2rad(variationX + x * (180.0 / maxX)))) +
                                     fabs(sin(deg2rad(variationY + y * (180.0 / maxY))));
            }
          }
        }
        else
        {
          for (int i = 1; i <= 100; i++)
          {
            dx[i] = rand();
            dy[i] = rand();

            if ( flag )
              dv[i] = rand();
            else
              dv[i] = 1;
          }
        }

        Chart_ewo2.setSeriesXYV(j, dx, dy, dv);
        Chart_ewo2.setSeriesName(j, \"series_\" + (string)j);
      }
      Chart_ewo2.setSeriesFillColor(0, colors[1]);
      Chart_ewo2.setSeriesFillColor(1, colors[3]);
      Chart_ewo2.setSeriesLineColor(0, colors[1]);
      Chart_ewo2.setSeriesLineColor(1, colors[3]);
      Chart_ewo2.setSeriesLineType(0, \"[none,oneColor,JoinMiter,CapButt,0]\");
      Chart_ewo2.setSeriesLineType(1, \"[none,oneColor,JoinMiter,CapButt,0]\");

      flag = !flag;
      break;

    case 7:  // Polar
      Chart_ewo2.chartType = \"Polar\";

      if ( VisibilitySeries0.state(0) )
        Chart_ewo2.setSeriesVisible(0, true);
      else
        Chart_ewo2.setSeriesVisible(0, false);

      if ( VisibilitySeries1.state(0) )
        Chart_ewo2.setSeriesVisible(1, true);
      else
        Chart_ewo2.setSeriesVisible(1, false);

      if ( (Chart_ewo2.seriesCount() > 2) &&
            Chart_ewo2.isSeriesVisible(2) )
        Chart_ewo2.setSeriesVisible(2, false);

      Chart_ewo2.removeAxes();

      Chart_ewo2.addAxis(0, 360, \"phi\");
      Chart_ewo2.addAxis(0, 35000, \"R\");

      dyn_float valuesX = makeDynFloat(0, 45, 90, 135, 180, 225, 270, 315);
      dyn_float randomY;

      for (int i = 1; i <= dynlen(valuesX); ++i)
        randomY[i] = rand();

      Chart_ewo2.setSeriesPolar(0, valuesX, randomY);

      dynClear(randomY);

      for (int i = 1; i <= dynlen(valuesX); ++i)
        randomY[i] = rand();

      Chart_ewo2.setSeriesPolar(1, valuesX, randomY);

      Chart_ewo2.setSeriesFillColor(0, \"{255,185,0,100}\");
      Chart_ewo2.setSeriesFillColor(1, \"{175,35,95,100}\");
      Chart_ewo2.setSeriesName(0, \"series 0\");
      Chart_ewo2.setSeriesName(1, \"series 1\");
      Chart_ewo2.setSeriesLineType(0, \"[none,oneColor,JoinMiter,CapButt,0]\");
      Chart_ewo2.setSeriesLineType(1, \"[none,oneColor,JoinMiter,CapButt,0]\");
      break;

  }
  Chart_ewo2.updatesEnabled = true;
}

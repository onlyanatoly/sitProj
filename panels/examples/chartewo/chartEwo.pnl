      values[i] = rand();
      labels[i] = \"value \" + (string)(i + 6);
    }

    Chart_ewo2.setSeries(1, values, labels);

//    for (int i = 1; i <= 6; i++)
//      Chart_ewo2.setItemFillColor(0, i-1, colors[i]);
  }

}

setVisiblity(int checkBoxIdx)
{
  status.text = \"\";
  switch ( checkBoxIdx )
  {
    case 0: // Donut
    case 1: // DonutBars
    {
      setValue(\"AngleSettings\", \"visible\", true);
      setValue(\"StopAngle\", \"visible\", true);
      setValue(\"DonutSettings\", \"visible\", true);
      Percent.visible(true);
      ValCol.visible(false);
      Grid.visible(false);
      Orientation.visible(false);
      setValue(\"MarkerSettings\", \"visible\", false);
      angleLabel.text = defaultLabel;

      break;
    }
    case 2: // Radar
    {
      setValue(\"AngleSettings\", \"visible\", true);
      setValue(\"StopAngle\", \"visible\", false);
      setValue(\"DonutSettings\", \"visible\", false);
      Percent.visible(false);
      ValCol.visible(true);
      Grid.visible(true);
      Orientation.visible(false);
      setValue(\"MarkerSettings\", \"visible\", true);
      if ( getActiveLang() == 0 )
        angleLabel.text = \"Drehung\";
      else
        angleLabel.text = \"Rotation\";

      break;
    }
    case 3: // Bars
    case 4: // StackedBars
    {
      setValue(\"AngleSettings\", \"visible\", false);
      setValue(\"DonutSettings\", \"visible\", false);
      Percent.visible(false);
      ValCol.visible(false);
      Grid.visible(true);
      Orientation.visible(true);
      setValue(\"MarkerSettings\", \"visible\", true);
      angleLabel.text = defaultLabel;

      break;
    }
    case 5: // PercentStackedBars
    {
      setValue(\"AngleSettings\", \"visible\", false);
      setValue(\"DonutSettings\", \"visible\", false);
      Percent.visible(true);
      ValCol.visible(false);
      Grid.visible(true);
      Orientation.visible(true);
      setValue(\"MarkerSettings\", \"visible\", false);
      angleLabel.text = defaultLabel;

      break;
    }
    case 6:  // Bubbles
    {
      setValue(\"AngleSettings\", \"visible\", false);
      setValue(\"DonutSettings\", \"visible\", false);
      Percent.visible(false);
      ValCol.visible(true);
      Grid.visible(true);
      Orientation.visible(false);
      setValue(\"MarkerSettings\", \"visible\", true);
      angleLabel.text = defaultLabel;

      break;
    }
    case 7: // Polar
    {
      setValue(\"AngleSettings\", \"visible\", true);
      setValue(\"StopAngle\", \"visible\", false);
      setValue(\"DonutSettings\", \"visible\", false);
      Percent.visible(false);
      ValCol.visible(false);
      Grid.visible(true);
      Orientation.visible(false);
      CHECK_BOX5.visible(true);
      setValue(\"MarkerSettings\", \"visible\", true);
      if ( getActiveLang() == 0 )
        angleLabel.text = \"Drehung\";
      else
        angleLabel.text = \"Rotation\";
    }
  }
}

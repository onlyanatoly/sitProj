clearAll()
{
  Chart_ewo2.removeAxes();

  int count = Chart_ewo2.seriesCount;
  for (int i = 0; i < count; i++)
     Chart_ewo2.removeSeries(0);
}

initRadar()
{
  Chart_ewo2.removeAxes();

  dyn_float values;
  dyn_string labels;

  for (int i = 1; i <= 6; i++)
  {
    values[i] = rand();
    labels[i] = \"value \" + (string)i;
    Chart_ewo2.addAxis(0, 32768, \"axis of\\nvalue \" + (string)i);
  }

  Chart_ewo2.setSeries(0, values, labels);

  for (int i = 1; i <= 6; i++)
  {
    Chart_ewo2.setItemFillColor(0, i-1, colors[i]);
  }

  //if ( Chart_ewo2.chartType == \"Radar\" )

"bool negative;

main(mapping event)
{
  dyn_float values;
  dyn_string labels;

  int count = 6;
  if ( Chart_ewo2.chartType == \"Radar\" )
    count = Chart_ewo2.axesCount;
  else if ( Chart_ewo2.chartType == \"Polar\" )
    count = 8;
  else
  {
    Chart_ewo2.removeAxes();
    Chart_ewo2.addAxis(0, 0, \"[EURO]\");
    Chart_ewo2.addAxis(0, 0, \"Values Last Year\");
  }

  for (int i = 1; i <= count; i++)
  {
    values[i] = rand();
    if ( negative &&
         ((Chart_ewo2.chartType == \"Bars\") ||
          (Chart_ewo2.chartType == \"StackedBars\")) )
      values[i] *= -1;

    labels[i] = \"Line \" + (string)i;
  }

  negative = !negative;

  Chart_ewo2.setSeries(1, values, labels);
  Chart_ewo2.setSeriesStretch(1, 2);

  for (int i = 1; i <= count; i++)
  {
    Chart_ewo2.setItemFillColor(1, i-1, colors[i]);
  }

  if ( Chart_ewo2.chartType == \"Polar\" )
    Chart_ewo2.setSeriesPolar(1, makeDynFloat(0, 45, 90, 135, 180, 225, 270, 315), values);
}


